	"scripts": {
		"dev:client": "react-scripts start",
		"dev:server": "nodemon server/index",
		"start": "node server/index",
		"start:prod": "NODE_ENV=production && node server/index",
		"db:seed": "node server/seeds/index",
		"deploy": "cd contract && ./deploy.sh",
		"build": "yarn build:contract && yarn build:web",
		"build:web": "react-scripts build",
		"build:contract": "cd contract && ./build.sh",
		"test": "yarn test:unit && yarn test:integration",
		"test:unit": "cd contract && cargo test",
		"test:integration": "cd integration-tests && cargo run --example integration-tests \"../contract/target/wasm32-unknown-unknown/release/hello_near.wasm\"",
		"postinstall": "echo rs tests && echo rs contract"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
